{
  "name": "code-lab",
  "displayName": "Code Lab",
  "description": "Interactive code execution and visualization - like Google Colab but for any file",
  "version": "1.0.0",
  "author": "Joshua Terranova",
  "publisher": "joshua-terranova",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Notebooks",
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeLab.executeSelection",
    "onCommand:codeLab.executeFunction",
    "onCommand:codeLab.showFlowChart",
    "onCommand:codeLab.showStructure",
    "onCommand:codeLab.inspectVariables",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeLab.executeSelection",
        "title": "‚ñ∂Ô∏è Execute Selection",
        "category": "Code Lab"
      },
      {
        "command": "codeLab.executeFunction",
        "title": "üöÄ Execute Function",
        "category": "Code Lab"
      },
      {
        "command": "codeLab.showFlowChart",
        "title": "üìä Show Flow Chart",
        "category": "Code Lab"
      },
      {
        "command": "codeLab.showStructure",
        "title": "üå≥ Show Structure",
        "category": "Code Lab"
      },
      {
        "command": "codeLab.inspectVariables",
        "title": "üîç Inspect Variables",
        "category": "Code Lab"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "codeLab.executeSelection",
          "group": "execution",
          "when": "editorHasSelection"
        },
        {
          "command": "codeLab.executeFunction",
          "group": "execution",
          "when": "editorHasSelection"
        },
        {
          "command": "codeLab.showFlowChart",
          "group": "visualization",
          "when": "editorHasSelection"
        },
        {
          "command": "codeLab.showStructure",
          "group": "visualization",
          "when": "editorHasSelection"
        },
        {
          "command": "codeLab.inspectVariables",
          "group": "inspection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "isolated-vm": "^4.6.1",
    "chart.js": "^4.4.0",
    "plotly.js": "^2.26.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
